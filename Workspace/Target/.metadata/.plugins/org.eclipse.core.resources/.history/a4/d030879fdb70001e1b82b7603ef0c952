#ifndef REGADDR_H_
#define REGADDR_H_

typedef struct{

	GPIO_A_EN	:1	;
	GPIO_B_EN	:1	;
	GPIO_C_EN	:1	;
	GPIO_D_EN	:1	;
	GPIO_E_EN	:1	;
	GPIO_F_EN	:1	;
	GPIO_G_EN	:1	;
	GPIO_H_EN	:1	;

	reserved_0	:4	;

	CRC_EN		:1	;

	reserved_1	:5	;

	BKP_SRAM_EN	:1	;

	reserved_2	:2	;

	DMA_1_EN	:1	;
	DMA_2_EN	:1	;

	reserved_3	:6	;

	OTGHS_EN	:1	;
	OTGHSULPI_EN:1	;

	reserved_4	:1	;

}RCC_AHB1ENR_t;

typedef struct{

}GPIOx_MODE_t;

typedef struct{

}GPIOx_ODR_t;

#endif /* REGADDR_H_ */
