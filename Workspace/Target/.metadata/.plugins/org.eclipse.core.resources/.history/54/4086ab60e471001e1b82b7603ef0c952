#include <stdint.h>

#ifndef REGADDR_H_
#define REGADDR_H_

typedef struct{

	uint32_t GPIO_A_EN		:1	;
	uint32_t GPIO_B_EN		:1	;
	uint32_t GPIO_C_EN		:1	;
	uint32_t GPIO_D_EN		:1	;
	uint32_t GPIO_E_EN		:1	;
	uint32_t GPIO_F_EN		:1	;
	uint32_t GPIO_G_EN		:1	;
	uint32_t GPIO_H_EN		:1	;

	uint32_t reserved_0		:4	;

	uint32_t CRC_EN			:1	;

	uint32_t reserved_1		:5	;

	uint32_t BKP_SRAM_EN	:1	;

	uint32_t reserved_2		:2	;

	uint32_t DMA_1_EN		:1	;
	uint32_t DMA_2_EN		:1	;

	uint32_t reserved_3		:6	;

	uint32_t OTGHS_EN		:1	;
	uint32_t OTGHSULPI_EN	:1	;

	uint32_t reserved_4		:1	;

} RCC_AHB1ENR_t;

typedef struct{

	uint32_t MODER_0 		:2	;
	uint32_t MODER_1 		:2	;
	uint32_t MODER_2 		:2	;
	uint32_t MODER_3 		:2	;
	uint32_t MODER_4 		:2	;
	uint32_t MODER_5 		:2	;
	uint32_t MODER_6 		:2	;
	uint32_t MODER_7 		:2	;
	uint32_t MODER_8 		:2	;
	uint32_t MODER_9 		:2	;
	uint32_t MODER_10 		:2	;
	uint32_t MODER_11 		:2	;
	uint32_t MODER_12 		:2	;
	uint32_t MODER_13 		:2	;
	uint32_t MODER_14 		:2	;
	uint32_t MODER_15 		:2	;

} GPIOx_MODER_t;

typedef struct{

	uint32_t IDR_0 			:2	;
	uint32_t IDR_1 			:2	;
	uint32_t IDR_2 			:2	;
	uint32_t IDR_3 			:2	;
	uint32_t IDR_4 			:2	;
	uint32_t IDR_5 			:2	;
	uint32_t IDR_6 			:2	;
	uint32_t IDR_7 			:2	;
	uint32_t IDR_8 			:2	;
	uint32_t IDR_9 			:2	;
	uint32_t IDR_10 		:2	;
	uint32_t IDR_11 		:2	;
	uint32_t IDR_12 		:2	;
	uint32_t IDR_13 		:2	;
	uint32_t IDR_14 		:2	;
	uint32_t IDR_15 		:2	;

	uint32_t reserved 		:16	;

} GPIOx_IDR_t;

typedef struct{

	uint32_t ODR_0 			:2	;
	uint32_t ODR_1 			:2	;
	uint32_t ODR_2 			:2	;
	uint32_t ODR_3 			:2	;
	uint32_t ODR_4 			:2	;
	uint32_t ODR_5 			:2	;
	uint32_t ODR_6 			:2	;
	uint32_t ODR_7 			:2	;
	uint32_t ODR_8 			:2	;
	uint32_t ODR_9 			:2	;
	uint32_t ODR_10 		:2	;
	uint32_t ODR_11 		:2	;
	uint32_t ODR_12 		:2	;
	uint32_t ODR_13 		:2	;
	uint32_t ODR_14 		:2	;
	uint32_t ODR_15 		:2	;

	uint32_t reserved 		:16	;

} GPIOx_ODR_t;

#endif /* REGADDR_H_ */
