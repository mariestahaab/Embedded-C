#ifndef REGADDR_H_
#define REGADDR_H_

typedef struct{

	uint32_t GPIO_A_EN		:1	;
	uint32_t GPIO_B_EN		:1	;
	uint32_t GPIO_C_EN		:1	;
	uint32_t GPIO_D_EN		:1	;
	uint32_t GPIO_E_EN		:1	;
	uint32_t GPIO_F_EN		:1	;
	uint32_t GPIO_G_EN		:1	;
	uint32_t GPIO_H_EN		:1	;

	uint32_t reserved_0		:4	;

	uint32_t CRC_EN			:1	;

	uint32_t reserved_1		:5	;

	uint32_t BKP_SRAM_EN	:1	;

	uint32_t reserved_2		:2	;

	uint32_t DMA_1_EN		:1	;
	uint32_t DMA_2_EN		:1	;

	uint32_t reserved_3		:6	;

	uint32_t OTGHS_EN		:1	;
	uint32_t OTGHSULPI_EN	:1	;

	uint32_t reserved_4		:1	;

}RCC_AHB1ENR_t;

typedef struct{

	uint32_t MODER0 		:2	;
	uint32_t MODER1 		:2	;
	uint32_t MODER2 		:2	;
	uint32_t MODER3 		:2	;
	uint32_t MODER4 		:2	;
	uint32_t MODER5 		:2	;
	uint32_t MODER6 		:2	;
	uint32_t MODER7 		:2	;
	uint32_t MODER8 		:2	;
	uint32_t MODER9 		:2	;
	uint32_t MODER10 		:2	;
	uint32_t MODER11 		:2	;
	uint32_t MODER12 		:2	;
	uint32_t MODER13 		:2	;
	uint32_t MODER14 		:2	;
	uint32_t MODER15 		:2	;

}GPIOx_MODER_t;

typedef struct{

}GPIOx_ODR_t;

#endif /* REGADDR_H_ */
