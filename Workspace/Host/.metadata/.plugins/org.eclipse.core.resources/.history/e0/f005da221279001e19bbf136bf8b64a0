#include <stdint.h>
#include <stdio.h>

int main(void){

	const uint32_t elements = 3;

	uint32_t items[elements] = {1, 2, 3};

	for(uint32_t i = 0; i < 3; i++){
		printf("%d\n", items[i]);
	}

	printf("Size of Array = %lu\n", sizeof(items));

	return 0;
}
