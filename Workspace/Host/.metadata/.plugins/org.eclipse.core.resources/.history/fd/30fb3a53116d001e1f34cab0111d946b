#include <stdint.h>
#include <stdio.h>

struct carModel{

	unsigned int carNumber;
	uint32_t	 carPrice;
	uint16_t	 carMaxSpeed;
	float		 carWeignt;
};

int main (void){

	// One Way:
	struct carModel carBMW = {2021, 15000, 220, 1330};	// 1 to 1 mapping.
	struct carModel carFord = {4031, 35000, 160, 1900.96};

	return 0;
}
