#include <stdint.h>
#include <stdio.h>

int main(void){

	uint8_t msg1[] = "Hello!";
	uint8_t msg2[] = {'H', 'e', 'l', 'l', 'o', '!'};

	printf("%s \n", msg1);
	printf("%lu \n", sizeof(msg1));

	printf("%s \n", msg2);
	printf("%lu \n", sizeof(msg2));

	return 0;
}
