#include <stdint.h>
#include <stdio.h>

int main(void){

	char *msg1 = "Hello!";

	uint32_t name[];

	printf("%s \n", msg1);		//stored in ROM.

	printf("Enter your name: ");
	scanf("%s", name);

	printf("Hi, %s!", name);

	return 0;
}
