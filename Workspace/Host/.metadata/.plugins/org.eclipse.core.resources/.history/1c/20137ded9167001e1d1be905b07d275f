long long int gData = 0x00007FFF8E3C3824;

#include <stdio.h>

int main(void){

	char *pAddress1;

	pAddress1 = (char*)&gData;

	printf ("Value = %llx\n", gData);

	printf ("Address = %p\n", pAddress1);

	printf ("Value at address (%p) = %hhx\n", pAddress1, *pAddress1);

	return 0;
}
