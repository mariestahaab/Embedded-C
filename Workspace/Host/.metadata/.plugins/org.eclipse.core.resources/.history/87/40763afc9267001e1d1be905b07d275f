long long int gData = 0x00007FFF8E3C3824;

#include <stdio.h>

int main(void){

	char *pAddress1;

	pAddress1 = (char*) &gData;
	printf ("Value at address (%p) = %x\n", pAddress1, *pAddress1);

	int *pAddress2 = (int*) &gData;
	printf ("Value at address (%p) = %x\n", pAddress2, *pAddress2);

	return 0;
}
